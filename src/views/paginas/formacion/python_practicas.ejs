<main class="code-container" role="main" aria-labelledby="titulo-practicas-python">
  <h1 id="titulo-practicas-python" class="page-title"><%= t["python.practices.title"] %></h1>

  <div id="content">
    <% const practicas = [
      { num: 1, archivos: ["01_Practica.py"] },
      { num: 2, archivos: ["02_practica.py"] },
      { num: 3, archivos: ["03_1practica.py", "03_2practica.py", "03_practica.py"] },
      { num: 4, archivos: ["04_practica.py"] },
      { num: 5, archivos: ["05_practica_v1.py", "05_practica_v2.py"] },
      { num: 6, archivos: ["06_practica.py", "06_practica_ferran.py"] },
      { num: 7, archivos: ["07_practicat.py"] },
      { num: 8, archivos: ["08_practica.py"] },
      { num: 9, archivos: ["09_practica.py"] },
      { num: 10, archivos: ["10_practica.py"] },
      { num: 11, archivos: ["11_practica.py"] },
      { num: 12, archivos: ["12_practica.py", "12_practica_v2.py", "12_practica_v3.py"] },
      { num: 13, archivos: ["13_practica.py"] },
      { num: 14, archivos: ["inventario.py"] }
    ]; %>

    <% practicas.forEach(({ num, archivos }) => { %>
      <details aria-label="<%= t['python.practices.label'] %> <%= num %>">
        <summary tabindex="0"><%= t["python.practices.label"] %> <%= num %></summary>
        <% archivos.forEach(archivo => { %>
          <pre><code class="language-python" data-src="/public/assets/programacion/python/practicas/ejecicios/<%= archivo %>"></code></pre>
        <% }) %>
      </details>
    <% }); %>
  </div>
</main>
